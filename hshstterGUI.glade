<?xml version="1.0"?>
<glade-interface>
  <!-- interface-requires gtk+ 2.6 -->
  <!-- interface-naming-policy toplevel-contextual -->
  <widget class="GtkWindow" id="mainWin">
    <property name="width_request">800</property>
    <property name="height_request">600</property>
    <property name="icon">./image/HshstterIcon.png</property>
    <child>
      <widget class="GtkVBox" id="vbox1">
        <property name="visible">True</property>
        <child>
          <widget class="GtkLabel" id="information">
            <property name="height_request">30</property>
            <property name="visible">True</property>
            <property name="xalign">0</property>
            <property name="yalign">0.89999997615814209</property>
            <property name="label" translatable="yes">label</property>
          </widget>
          <packing>
            <property name="expand">False</property>
            <property name="position">0</property>
          </packing>
        </child>
        <child>
          <widget class="GtkHBox" id="hbox">
            <property name="visible">True</property>
            <child>
              <widget class="GtkEntry" id="tweetEntry">
                <property name="height_request">30</property>
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="invisible_char">&#x25CF;</property>
              </widget>
              <packing>
                <property name="position">0</property>
              </packing>
            </child>
            <child>
              <widget class="GtkButton" id="tweetButton">
                <property name="label" translatable="yes">tweet</property>
                <property name="width_request">60</property>
                <property name="height_request">30</property>
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="receives_default">True</property>
              </widget>
              <packing>
                <property name="expand">False</property>
                <property name="fill">False</property>
                <property name="position">1</property>
              </packing>
            </child>
          </widget>
          <packing>
            <property name="expand">False</property>
            <property name="fill">False</property>
            <property name="position">1</property>
          </packing>
        </child>
        <child>
          <widget class="GtkScrolledWindow" id="timelineWin">
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="hscrollbar_policy">automatic</property>
            <property name="vscrollbar_policy">automatic</property>
            <child>
              <placeholder/>
            </child>
          </widget>
          <packing>
            <property name="position">2</property>
          </packing>
        </child>
      </widget>
    </child>
  </widget>
  <widget class="GtkWindow" id="accessTokenGetWin">
    <property name="width_request">440</property>
    <property name="height_request">250</property>
    <property name="resizable">False</property>
    <child>
      <widget class="GtkFixed" id="authorizationField">
        <property name="visible">True</property>
        <child>
          <widget class="GtkLabel" id="hint">
            <property name="width_request">366</property>
            <property name="height_request">34</property>
            <property name="visible">True</property>
            <property name="label" translatable="yes">Please click the following link
and access OAuth's authorization page.</property>
          </widget>
          <packing>
            <property name="x">42</property>
            <property name="y">20</property>
          </packing>
        </child>
        <child>
          <widget class="GtkEntry" id="pinEntry">
            <property name="width_request">400</property>
            <property name="height_request">30</property>
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="invisible_char">&#x25CF;</property>
          </widget>
          <packing>
            <property name="x">28</property>
            <property name="y">121</property>
          </packing>
        </child>
        <child>
          <widget class="GtkButton" id="authorizationButton">
            <property name="label" translatable="yes">Authorization</property>
            <property name="width_request">100</property>
            <property name="height_request">40</property>
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="receives_default">True</property>
          </widget>
          <packing>
            <property name="x">115</property>
            <property name="y">159</property>
          </packing>
        </child>
        <child>
          <widget class="GtkButton" id="cancelButton">
            <property name="label" translatable="yes">Cancel</property>
            <property name="width_request">100</property>
            <property name="height_request">40</property>
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="receives_default">True</property>
          </widget>
          <packing>
            <property name="x">222</property>
            <property name="y">159</property>
          </packing>
        </child>
        <child>
          <widget class="GtkLabel" id="label2">
            <property name="width_request">314</property>
            <property name="height_request">20</property>
            <property name="visible">True</property>
            <property name="xalign">0</property>
            <property name="yalign">1</property>
            <property name="label" translatable="yes">Input PIN code</property>
          </widget>
          <packing>
            <property name="x">28</property>
            <property name="y">98</property>
          </packing>
        </child>
      </widget>
    </child>
  </widget>
</glade-interface>
